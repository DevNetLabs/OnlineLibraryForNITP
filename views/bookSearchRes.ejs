<%- include('partials/header') %>


    
    <div class="ui container">
        <h6>Book Search Results</h6>
        <h3>Welcome <%= name %> </h3>
        <p>Here is the search result for your book search.</p>

        <table class="ui celled table">
            <caption>Found <%=results.length%> Results</caption>
            <tr>
                <th scope="col">Book_Id</th>
                <th scope="col">Book_Name</th>
                <th scope="col">Book_Author</th>
            </tr>

            <% for(var i=0; i < results.length; i++) { %>
            <tr>
            <td data-label="Book_Id" scope="row"><%= results[i].bookId %></td>
            <td data-label="Book_Name "><%= results[i].bookName %></td>
            <td data-label="Book_Author"><%= results[i].bookAuthor %></td>
            </tr>
            <% } %>
            
        </table>
       
        <a class="large ui inverted primary button" href="/logout" role="button">Logout</a>
    </div>


    






<%- include('partials/footer') %>

